<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="pct_core_secur.wsse_auth.class">PCT\Core\SecurBundle\Security\Auth\UserPassAuth</parameter>
    </parameters>

    <services>
        <service id="pct_core_secur.wsse_auth" class="%pct_core_secur.wsse_auth.class%">
      <argument type="service" id="security.context"/>
      <argument type="service" id="session"/>
      <argument type="service" id="fos_user.user_manager"/>
      <argument type="service" id="fos_user.user_provider.username_email"/>
      <argument type="service" id="security.encoder_factory"/>
        </service>
    </services>

</container>